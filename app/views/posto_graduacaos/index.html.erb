<p id="notice"><%= notice %></p>

<h1>Listing Posto Graduacaos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posto_graduacaos.each do |posto_graduacao| %>
      <tr>
        <td><%= posto_graduacao.name %></td>
        <td><%= link_to 'Show', posto_graduacao %></td>
        <td><%= link_to 'Edit', edit_posto_graduacao_path(posto_graduacao) %></td>
        <td><%= link_to 'Destroy', posto_graduacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posto graduacao', new_posto_graduacao_path %>
