<p id="notice"><%= notice %></p>

<h1>Listing Graduations</h1>

<table>
  <thead>
    <tr>
      <th>Pgdesc</th>
      <th>Pgabrev</th>
      <th>Pgabrevtdp</th>
      <th>Pgrefeitorio</th>
      <th>Pggrupo1</th>
      <th>Pggrupotdp</th>
      <th>Pgclasse</th>
      <th>Pgid</th>
      <th>Pggrupo2</th>
      <th>Pgpronome</th>
      <th>Pgpronome2</th>
      <th>Pgabrev1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graduations.each do |graduation| %>
      <tr>
        <td><%= graduation.pgdesc %></td>
        <td><%= graduation.pgabrev %></td>
        <td><%= graduation.pgabrevtdp %></td>
        <td><%= graduation.pgrefeitorio %></td>
        <td><%= graduation.pggrupo1 %></td>
        <td><%= graduation.pggrupotdp %></td>
        <td><%= graduation.pgclasse %></td>
        <td><%= graduation.pgid %></td>
        <td><%= graduation.pggrupo2 %></td>
        <td><%= graduation.pgpronome %></td>
        <td><%= graduation.pgpronome2 %></td>
        <td><%= graduation.pgabrev1 %></td>
        <td><%= link_to 'Show', graduation %></td>
        <td><%= link_to 'Edit', edit_graduation_path(graduation) %></td>
        <td><%= link_to 'Destroy', graduation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graduation', new_graduation_path %>
